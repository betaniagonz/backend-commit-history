{
  "version":2,
  "builds":[
    {
      "src":"src/main.ts",
      "use":"@vercel/node"
    }
  ],
  "routes":[
    {
      "src":"/(.*)",
      "dest":"src/main.ts",
      "methods":[
        "GET",
        "POST",
        "PUT",
        "DELETE"
      ]
    },
    {
      "src": "/doc/(.*)",
      "dest": "/doc/$1",
      "methods":[
        "GET",
        "POST",
        "PUT",
        "DELETE"
      ]
    }
  ]
}